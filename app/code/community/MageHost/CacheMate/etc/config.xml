<!--suppress XmlUnboundNsPrefix, CheckTagEmptyBody -->
<config>
    <modules>
        <MageHost_CacheMate>
            <version>1.20.2</version>
        </MageHost_CacheMate>
    </modules>
    <global>
        <blocks>
            <magehost_cachemate>
                <class>MageHost_CacheMate_Block</class>
            </magehost_cachemate>
        </blocks>
        <models>
            <magehost_cachemate>
                <class>MageHost_CacheMate_Model</class>
            </magehost_cachemate>
        </models>
        <helpers>
            <magehost_cachemate>
                <class>MageHost_CacheMate_Helper</class>
            </magehost_cachemate>
        </helpers>
        <events>
            <!-- MageHost_Cm_Cache_Backend_XXX -->
            <magehost_clean_backend_cache_before>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cleanBackendCache</method>
                    </magehost_cachemate>
                </observers>
            </magehost_clean_backend_cache_before>
            <!-- Core -->
            <adminhtml_cache_flush_all>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </adminhtml_cache_flush_all>
            <adminhtml_cache_flush_system>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </adminhtml_cache_flush_system>
            <adminhtml_cache_refresh_type>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </adminhtml_cache_refresh_type>
            <clean_media_cache_after>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </clean_media_cache_after>
            <clean_catalog_images_cache_after>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </clean_catalog_images_cache_after>
            <clean_configurable_swatches_cache_after>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </clean_configurable_swatches_cache_after>
            <application_clean_cache>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </application_clean_cache>
            <core_clean_cache>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </core_clean_cache>
            <!-- Enterprise Core -->
            <clean_cache_by_tags>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </clean_cache_by_tags>
            <!-- Turpentine -->
            <turpentine_varnish_flush_all>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </turpentine_varnish_flush_all>
            <turpentine_varnish_flush_partial>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </turpentine_varnish_flush_partial>
            <turpentine_varnish_flush_content_type>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </turpentine_varnish_flush_content_type>
            <turpentine_ban_product_cache>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </turpentine_ban_product_cache>
            <turpentine_ban_product_cache_check_stock>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </turpentine_ban_product_cache_check_stock>
            <turpentine_ban_category_cache>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </turpentine_ban_category_cache>
            <turpentine_ban_media_cache>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </turpentine_ban_media_cache>
            <turpentine_ban_catalog_images_cache>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </turpentine_ban_catalog_images_cache>
            <turpentine_ban_cms_page_cache>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </turpentine_ban_cms_page_cache>
            <turpentine_ban_all_cache>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </turpentine_ban_all_cache>
            <turpentine_ban_esi_cache>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>cacheFlushEvent</method>
                    </magehost_cachemate>
                </observers>
            </turpentine_ban_esi_cache>
        </events>
    </global>
    <frontend>
        <events>
            <core_block_abstract_to_html_before>
                <observers>
                    <magehost_cachemate>
                        <type>singleton</type>
                        <class>magehost_cachemate/observer</class>
                        <method>coreBlockAbstractToHtmlBefore</method>
                    </magehost_cachemate>
                </observers>
            </core_block_abstract_to_html_before>
        </events>
    </frontend>
    <default>
        <magehost_cachemate>
            <logging>
                <flushes>0</flushes>
                <tags>0</tags>
            </logging>
            <flushes>
                <catalog_category>1</catalog_category>
                <catalog_product>1</catalog_product>
                <cms_page>1</cms_page>
                <cms_block>1</cms_block>
                <translate>0</translate>
                <store>0</store>
                <website>0</website>
                <block_html>0</block_html>
                <mage>0</mage>
                <_without_tags>0</_without_tags>
            </flushes>
        </magehost_cachemate>
    </default>
</config>
